import java.util.Scanner;
class reverse {
    public static void main(String[] args){
        int i =1; int n = 5; int a=0;
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the string value: ");
        int num = sc.nextInt();
        while( i <= n) {
            int m = num % 10;
             num = num/10;
            a= a * 10 + m;
            i++;
        }
        System.out.println("reverse value is : " +a);
    }
}
